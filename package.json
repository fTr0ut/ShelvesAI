    "@plasmicpkgs/react-aria": "^0.0.153",
    "axios": "^1.11.0",
    "concurrently": "^9.0.0",
    "expo": "^54.0.8",
    "jest": "^30.1.3",
    "mongodb-memory-server": "^10.2.1",
    "openai": "^5.20.3",
    "path": "^0.12.7",
    "supertest": "^7.1.4"
  },
  "devDependencies": {
    "@plasmicapp/cli": "^0.1.345",
    "cross-env": "^10.0.0"
  }
}
